{
    "name": "ydg/hyperf-admin-core",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "hyperf",
        "hyperf admin",
        "hyperf admin core"
    ],
    "authors": [
        {
            "name": "ydg",
            "email": "1028180290@qq.com",
            "role": "Developer"
        }
    ],
    "description": "hyperf admin core code",
    "autoload": {
        "psr-4": {
            "HyperfAdminCore\\": "src/"
        },
        "files": [
            "src/Helper/functions.php"
        ]
    },
    "require": {
        "php": ">=8.1",
        "hyperf/async-queue": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/db-connection": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/framework": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/logger": "~3.1.0",
        "hyperf/model-cache": "~3.1.0",
        "hyperf/paginator": "~3.1.0",
        "hyperf/redis": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/validation": "~3.1.0",
        "zoujingli/ip2region": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/devtool": "~3.1.0"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "HyperfAdminCore\\ConfigProvider"
        }
    }
}
